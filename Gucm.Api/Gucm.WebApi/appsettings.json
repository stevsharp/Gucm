{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "",
    "ODataDBConnection": "",
    "Elastic": "http://51.140.72.239:9200"
  },
  "Serilog": {
    // Log levels include:
    //     Verbose, Debug, Information, Warning, Error, Fatal
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        // Color-coded structured logging to console
        "Name": "LiterateConsole",
        "Args": {
          // The optional outputTemplate defines the format of logged messages and provides
          // a way to log additional fields (like ASP.NET Core's RequestId) which may not
          // be included in default message templates.
          "outputTemplate": "{Timestamp:HH:mm:ss} {Level} | {RequestId} - {Message}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "Application": "WebApplication6",
      "MachineName": "localhost"
    }
  },
  "HealthChecks-UI": {
    "HealthChecks": [
      {
        "Name": "Yield Api",
        "Uri": "http://localhost:13231/health"
      },
      {
        "Name": "Contracts Api",
        "Uri": "http://localhost:13231/health"
      }
    ],
    "Webhooks": [
      {
        "Name": "",
        "Uri": "",
        "Payload": "",
        "RestoredPayload": ""
      }
    ],
    "EvaluationTimeOnSeconds": 10,
    "MinimumSecondsBetweenFailureNotifications": 60
  }
}
